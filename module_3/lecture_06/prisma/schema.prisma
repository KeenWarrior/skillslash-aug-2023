model ParkingLot {
    id    Int    @id @default(autoincrement())
    name  String
    partkingSpots PartkingSpot[]
}

model PartkingSpot {
    id    Int    @id @default(autoincrement())
    parkingLot ParkingLot @relation(fields: [parkingLotId], references: [id])
    parkingLotId Int
    floorNumber Int
    vehicleType VehicleType @default(TWO_VEHILER)
}

enum VehicleType {
    TWO_VEHILER
    THREE_VEHILER
    FOUR_VEHILER
}